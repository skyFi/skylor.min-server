{
  "name": "egg-react",
  "version": "0.0.1",
  "description": "egg-react application",
  "dependencies": {
    "egg": "^2.0.0",
    "egg-mysql": "^3.0.0",
    "egg-passport": "^2.0.1",
    "egg-passport-github": "^1.0.0",
    "egg-scripts": "^2.1.0",
    "egg-validate": "^1.0.0",
    "egg-view-ejs": "^2.0.0",
    "os": "^0.1.1",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-router": "^4.2.0"
  },
  "devDependencies": {
    "autod": "^3.0.1",
    "autod-egg": "^1.0.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-regenerator": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-3": "^6.24.1",
    "babel-register": "^6.26.0",
    "babel-runtime": "^6.26.0",
    "browser-sync": "^2.23.2",
    "cli-color": "^1.2.0",
    "commitizen": "^2.9.6",
    "commitlint": "^5.2.8",
    "cz-conventional-changelog": "^2.1.0",
    "del": "^3.0.0",
    "egg-bin": "^4.3.5",
    "egg-ci": "^1.8.0",
    "egg-mock": "^3.13.0",
    "eslint": "^4.11.0",
    "eslint-config-egg": "^5.1.0",
    "gulp": "^3.9.1",
    "gulp-sequence": "^1.0.0",
    "happypack": "^4.0.0",
    "lint-staged": "^6.0.0",
    "standard-changelog": "^1.0.7",
    "standard-version": "^4.3.0",
    "uglifyjs-webpack-plugin": "^1.1.5",
    "webpack": "^3.10.0",
    "webstorm-disable-index": "^1.2.0"
  },
  "engines": {
    "node": ">=8.9.0"
  },
  "scripts": {
    "start": "egg-scripts start --daemon",
    "stop": "egg-scripts stop",
    "dev": "egg-bin dev",
    "debug": "egg-bin debug",
    "test": "npm run lint -- --fix && npm run test-local",
    "test-local": "egg-bin test",
    "cov": "egg-bin cov",
    "commit-release:install": "yarn add commitlint lint-staged standard-version standard-changelog commitizen cz-conventional-changelog --dev",
    "lint": "eslint --ignore-path .gitignore .",
    "lint:fix": "eslint --ignore-path .gitignore --fix .",
    "precommit": "lint-staged",
    "commitmsg": "commitlint -e $GIT_PARAMS",
    "git:commit": "git add . && git cz",
    "git:push": "git push",
    "git:push:after-release": "git push --follow-tags origin master",
    "release": "standard-version --commit-all",
    "fly": "npm run git:commit && npm run git:push",
    "fly:release": "git checkout master && git pull && yarn && npm run release && npm run git:push:after-release",
    "ci": "npm run lint && npm run cov",
    "autod": "autod",
    "pid": "ps -eo \"pid,command\" | grep \"title=egg-server-egg-react\"",
    "webpack": "webpack --config frontend/webpack.vendors.config.js && webpack --config frontend/webpack.app.config.js && ",
    "webpack:app": "webpack --config frontend/webpack.app.config.js",
    "webpack:vendors": "webpack --config frontend/webpack.vendors.config.js",
    "gulp:dev": "gulp dev --gulpfile frontend/gulpfile.js"
  },
  "ci": {
    "version": "8"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/skyFi/skylor.min-server.git"
  },
  "author": "skylor",
  "license": "MIT",
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "standard-version": {
    "scripts": {
      "postbump": "git add ."
    }
  }
}
